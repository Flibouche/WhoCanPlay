{% extends 'base.html.twig' %}

{% block title %}Hello FeatureController!{% endblock %}

{% block body %}

<form action="{{ path('search_api_game') }}" method="GET" enctype="multipart/form-data" id="search-form">   
    <label for="search" class="sr-only">Search</label>
    <div>
        <input type="search" name="game" id="search"  placeholder="Search by name" required />
        <div id="result"></div>
    </div>
    <button type="submit">
        <svg class="w-4 h-4" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 20 20">
            <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="m19 19-4-4m0-7A7 7 0 1 1 1 8a7 7 0 0 1 14 0Z"/>
        </svg>
        <span class="sr-only">Search</span>
    </button>
</form>

{% endblock %}
